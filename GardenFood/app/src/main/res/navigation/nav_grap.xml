<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_grap"
    app:startDestination="@id/menuFragment">

    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.gardenfood.ui.food.menu.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_menuFragment_to_foodFragment"
            app:destination="@id/foodFragment" />
        <action
            android:id="@+id/action_menuFragment_to_newFoodFragment"
            app:destination="@id/newFoodFragment" />
    </fragment>
    <fragment
        android:id="@+id/newFoodFragment"
        android:name="com.example.gardenfood.ui.food.NewFoodFragment"
        android:label="fragment_new_food"
        tools:layout="@layout/fragment_new_food" />
    <fragment
        android:id="@+id/foodFragment"
        android:name="com.example.gardenfood.ui.food.FoodFragment"
        android:label="FoodFragment" />
</navigation>